<div class="modal fade" id="new-route-alert" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header" id="modal-loading-header">
                <h3 class="modal-title">New Route</h3>
            </div>
            <div class="modal-body" id="new-route-body">
                <form class="form-horizontal" role="form" id="new-route-form">
                    <div class="form-group">
                        <label for="inputRouteName" class="col-sm-2 control-label"><strong>Route Name</strong></label>
                        <div class="col-sm-15 new-route-input">
                            <input class="form-control" type="text" id="inputRouteName" placeholder="Enter route name">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="inputRouteType" class="col-sm-2 control-label"><strong>Route Type</strong></label>
                        <div class="col-sm-15 new-route-input">
                            <select class="form-control" id="inputRouteType">
                                <option value=""> </option>
                                <option value="bus">Bus</option>
                                <option value="subway">Subway</option>
                            </select>
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer" id="new-route-footer">
                <button type="button" id="cancel-route-btn" class="btn btn-default" data-dismiss="modal">Cancel</button>
                <button type="button" id="create-route-btn" class="btn btn-primary" >Build Route</button>
            </div>
        </div>
    </div>
</div>