<h3>Select Layers</h3>
<form  id="map-layer-ctrl">
    <% for (var layer in layers) { %>
    <%  if(!layers.hasOwnProperty(layer)) { %>
    <%      continue; } %>
    <div class="checkbox">
        <label>
            <input type="checkbox" name=<%= layer %> value=<%= layer %>
                    <%= layers[layer].toggled ? 'checked' : ''  %>>
            <%= layers[layer].name %>
        </label>
    </div>
    <% } %>
</form>